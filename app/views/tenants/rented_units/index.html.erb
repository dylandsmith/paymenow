<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Rented Units</h3>
  </div>
  <div class="panel-body">
    <% @rented_units.each do |p| %>
      <div class="row">
        <div class="col-md-12">
          <span><%= p.address %></span><span><%= p.leases.first.debt %></span>
        </div>
      </div>
    <% end %>
  </div>
</div>