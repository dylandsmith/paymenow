<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Managed Properties</h3>
  </div>
  <div class="panel-body">
    <% @properties.each do |p| %>
      <div class="row">
        <div class="col-md-12">
          <%= link_to p.title, managers_property_path(p.id) %>
        </div>
      </div>
    <% end %>
  </div>
</div>


<%= form_for :property do |f|%>
  <%= f.label(:title, 'Property Title') %>
  <%= f.text_field(:title, class: 'form-control') %>
  <%= f.submit('Add Property', class: 'form-control') %>
<% end %>